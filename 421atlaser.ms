		fn fn_checkForBitmap obj =
		(
			if obj.material != undefined and obj.material.DiffuseMap != undefined then
			(
				if classof obj.material.DiffuseMap == BitmapTexture then
				(
					labelDebugText += ("\nFound bitmap texture: "+ obj.material.DiffuseMap.filename as string +" \non object: " +obj.name)
				return true
				)
