			labelDebugText += ("\nNo texture on " +obj.name)
			return false
		)
		
		fn fn_editUV obj corner =
		(
			select  (execute ("$'"+ obj.name + "'"))
			modPanel.addModToSelection(Edit_Poly()) ui:on
            local editPoly = $.modifiers[#edit_poly]
			local allFaces = #{1.. polyop.getNumFaces obj}
			modPanel.addModToSelection (Unwrap_UVW ()) ui:on
            local uv_modifier = $.modifiers[#unwrap_uvw]                
            uv_modifier.unwrap2.setTVSubObjectMode 3 
                 
           -- if (aUVChannel != 1) then 
           -- (
            --    uv_modifier.unwrap.setMapChannel aUVChannel
           --     uv_modifier.unwrap.reset()
           --     forcecompleteredraw dodisabled:true -- Hacky fix for a bug, see http://www.polycount.com/forum/showthread.php?t=97059
            --)
				uv_modifier.unwrap2.selectFaces allfaces
			scaleFactor = 0.5
				uv_modifier.unwrap2.ScaleSelectedXY scaleFactor scaleFactor [0,0,0]
				moveY = 0.5
				moveX = 0.5
				--tempStr = ("")
				--labelDebugText = ""
		
				if (corner == 1) then 
				(
					uv_modifier.unwrap2.moveSelected [0,moveY,0]
