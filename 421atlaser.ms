					btn_resNew.text = ("New Tex:\n[ " +newRes as string+ "px x " +newRes as string+ "px ]")					)
			
		fn fn_getTexture obj texIndex =
		(
			canWeGo = false
			canWeGo = fn_checkForBitmap obj
			if (canWeGo) then
			(
				tempBitmap = obj.material.DiffuseMap
				arr_WorkTexture[texIndex] = tempBitmap
