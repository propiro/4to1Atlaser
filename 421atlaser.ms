				labelDebugText += (", stored in " + texIndex as string + " slot.")
				fn_populateButtonImages arr_workTextureFilename[texIndex] texIndex
				fn_showBitmapResolution arr_workTextureFilename[texIndex] texIndex
				fn_showMeshData arr_WorkObject[texIndex] texIndex
				fn_calculateNewMeshTex()
			)
		)
		
		
	
		
